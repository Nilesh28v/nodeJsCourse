<h1>Number Of Registerd Users <%= student.length %><br>
        <a href='/getform'>Add User</a><br>
        <table border="1">
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
              
            </tr>
            <% for (var i=0; i< JSON.stringify(student.length); i++){ %> 
            <tr>
                <td>
                    <%=student[i].fname %>
                </td>
                <td>
                    <%=student[i].lname %>
                </td>
                <td>
                    <%=student[i].email %>
                </td>
        
            </tr>
                <% } %>

        </table><br>
</h1>